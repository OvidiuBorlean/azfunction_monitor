{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "outputBlob",
      "path": "out/{rand-guid}",
      "connection": "OvidiuStorage",
      "direction": "out",
      "type": "blob"
    }
  ]
}